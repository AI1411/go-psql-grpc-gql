type Task {
    id: Int!
    title: String!
    description: String!
    dueDate: String!
    completed: Boolean!
    user_id: Int!
    status: String!
    createdAt: String!
    updatedAt: String!
}

input ListTaskInput {
    title: String
    completed: Boolean
    user_id: Uint32
    status: String
}

input CreateTaskInput {
    title: String!
    description: String
    dueDate: String
    user_id: Int
    status: String!
}

scalar Uint32