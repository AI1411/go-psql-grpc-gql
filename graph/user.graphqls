type User {
    id: Int!
    name: String!
    email: String!
    password: String!
    created_at: String!
    updated_at: String!
}

type ChangePasswordResponse {
    password: String!
}

input CreateUserInput {
    name: String!
    email: String!
    password: String!
}

input UpdateUserInput {
    id: Int!
    name: String
    email: String
    password: String
}

input ChangePasswordInput {
    id: Int!
    old_password: String!
    new_password: String!
}